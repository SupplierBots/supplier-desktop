{
  "name": "safedropbot-dev",
  "productName": "SafeDrop-Dev",
  "description": "Supreme & Palace Bot",
  "version": "1.0.0",
  "copyright": "SafeDrop",
  "main": "build/electron.js",
  "homepage": ".",
  "private": true,
  "author": {
    "name": "Your Name",
    "email": "your.email@domain.com",
    "url": "https://your-website.com"
  },
  "scripts": {
    "start": "NODE_ENV=development yarn build-main && concurrently \"cross-env BROWSER=none craco start\" \"wait-on http://localhost:3000 && electron .\" --kill-others --raw",
    "production": "NODE_ENV=production electron .",
    "build": "yarn build-main && yarn build-renderer && yarn compile",
    "build-main": "NODE_ENV=production webpack --config src/main/webpack.config.js && cp public/app.js build/app.js && rm -f build/app.jsc",
    "build-renderer": "cross-env GENERATE_SOURCEMAP=false NODE_ENV=production craco build",
    "compile": "node tools/compile.js",
    "prepack": "node tools/prepack.js",
    "postinstall": "patch-package && NODE_ENV=development yarn build-main && electron-builder install-app-deps",
    "package-all": "yarn prepack && NODE_ENV=production lectron-builder build -mw",
    "package-mac": "yarn prepack && NODE_ENV=production electron-builder build -m",
    "package-win": "yarn prepack && NODE_ENV=production electron-builder build -w"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,css,scss}": [
      "prettier --config .prettierrc --write",
      "eslint --fix",
      "stylelint 'src/**/*'",
      "git add"
    ]
  },
  "build": {
    "appId": "com.my-website.electron-cra-ts-redux-router",
    "productName": "Electron CRA-TS-Redux-Router",
    "copyright": "Copyright Â© 2019 ${author}",
    "mac": {
      "category": "public.app-category.utilities",
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources"
    }
  },
  "dependencies": {
    "@craco/craco": "^5.6.3",
    "@types/aes-js": "^3.1.0",
    "@types/crypto-js": "^3.1.43",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "bytenode": "^1.1.6",
    "cheerio": "^1.0.0-rc.3",
    "connected-react-router": "^6.6.1",
    "crypto-js": "^3.1.9-1",
    "electron-better-ipc": "^0.8.0",
    "electron-is-dev": "^1.1.0",
    "firebase": "^7.8.0",
    "formik": "^2.1.3",
    "fs-extra": "^8.1.0",
    "history": "^4.10.1",
    "path-to-regexp": "^6.1.0",
    "puppeteer": "1.17.0",
    "puppeteer-extra": "3.1.9",
    "puppeteer-extra-plugin": "3.1.4",
    "puppeteer-extra-plugin-stealth": "2.4.9",
    "ramda": "^0.27.0",
    "react": "^16.12.0",
    "react-dom": "npm:@hot-loader/react-dom",
    "react-hot-loader": "^4.12.19",
    "react-input-mask": "^3.0.0-alpha.2",
    "react-is": "^16.12.0",
    "react-particles-js": "^2.7.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.1",
    "react-select": "^3.0.8",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-persist-transform-encrypt": "^2.0.1",
    "redux-saga": "^1.1.3",
    "redux-saga-firebase": "^0.15.0",
    "request-promise-native": "^1.0.8",
    "unlazy-loader": "^0.1.3",
    "uuid": "^3.4.0",
    "yup": "^0.28.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@types/cheerio": "^0.22.16",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^25.1.1",
    "@types/node": "^13.7.0",
    "@types/puppeteer": "^2.0.0",
    "@types/ramda": "^0.26.41",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-input-mask": "^2.0.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-select": "^3.0.10",
    "@types/request-promise-native": "^1.0.17",
    "@types/styled-components": "^4.4.2",
    "@types/uuid": "^3.4.7",
    "@types/yup": "^0.26.29",
    "babel-plugin-styled-components": "^1.10.7",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.0",
    "electron": "^8.1.0",
    "electron-builder": "^22.4.0",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "node-sass": "^4.13.1",
    "patch-package": "^6.2.0",
    "postinstall-postinstall": "^2.0.0",
    "prettier": "^1.19.1",
    "string-replace-loader": "^2.2.0",
    "styled-components": "^5.0.0",
    "stylelint": "^13.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.9.0",
    "typescript": "^3.8.2",
    "wait-on": "^4.0.0",
    "webpack-cli": "^3.3.11",
    "webpack-messages": "^2.0.4",
    "webpack-node-externals": "^1.7.2"
  },
  "browserslist": [
    "electron >=8"
  ]
}
