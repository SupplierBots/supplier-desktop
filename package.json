{
  "name": "safedropbot-dev",
  "productName": "SafeDrop-Dev",
  "description": "Supreme & Palace Bot",
  "version": "1.0.0",
  "copyright": "SafeDrop",
  "main": "app/index.html",
  "homepage": ".",
  "private": true,
  "window": {
    "title": "SafeDrop",
    "toolbar": true,
    "frame": false,
    "width": 1200,
    "height": 750,
    "resizable": false,
    "show": false
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development concurrently \"cross-env BROWSER=none craco start\" \"wait-on http://localhost:3000 && ./NW.js/nwjs.app/Contents/MacOS/nwjs ./dev-server\" --kill-others --raw",
    "build": "cross-env GENERATE_SOURCEMAP=false craco build",
    "test": "craco test",
    "run-prod": "./NW.js/nwjs.app/Contents/MacOS/nwjs .",
    "compile": "yarn build && node compile",
    "start-win": "cross-env NODE_ENV=development concurrently \"cross-env BROWSER=none craco start\" \"wait-on http://localhost:3000 && ./NW.js/nw ./dev-server\" --kill-others --raw",
    "run-prod-win": "yarn compile && ./NW.js/nw ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,css,scss}": [
      "prettier --config .prettierrc --write",
      "eslint --fix",
      "stylelint 'src/**/*'",
      "git add"
    ]
  },
  "build": {
    "nwVersion": "0.43.6",
    "files": [
      "app/**/*"
    ],
    "win": {
      "productName": "SafeDrop-Dev",
      "companyName": "SafeDrop-Dev",
      "copyright": "SafeDrop-Dev",
      "icon": "./resources/icons/icon.ico"
    },
    "mac": {
      "name": "SafeDrop-Dev",
      "displayName": "SafeDrop-Dev",
      "copyright": "SafeDrop-Dev",
      "icon": "./resources/icons/icon.icns"
    }
  },
  "dependencies": {
    "@craco/craco": "^5.6.3",
    "@types/aes-js": "^3.1.0",
    "@types/crypto-js": "^3.1.43",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "cheerio": "^1.0.0-rc.3",
    "connected-react-router": "^6.6.1",
    "crypto-js": "^3.1.9-1",
    "firebase": "^7.8.0",
    "formik": "^2.1.3",
    "fs-extra": "^8.1.0",
    "history": "^4.10.1",
    "path-to-regexp": "^6.1.0",
    "puppeteer": "1.17.0",
    "puppeteer-extra": "3.1.9",
    "puppeteer-extra-plugin": "3.1.4",
    "puppeteer-extra-plugin-stealth": "2.4.9",
    "ramda": "^0.27.0",
    "react": "^16.12.0",
    "react-dom": "npm:@hot-loader/react-dom",
    "react-hot-loader": "^4.12.19",
    "react-input-mask": "^3.0.0-alpha.2",
    "react-is": "^16.12.0",
    "react-particles-js": "^2.7.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.1",
    "react-select": "^3.0.8",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-persist-transform-encrypt": "^2.0.1",
    "redux-saga": "^1.1.3",
    "redux-saga-firebase": "^0.15.0",
    "request-promise-native": "^1.0.8",
    "unlazy-loader": "^0.1.3",
    "uuid": "^3.4.0",
    "yup": "^0.28.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@types/cheerio": "^0.22.16",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^25.1.1",
    "@types/node": "^13.7.0",
    "@types/puppeteer": "^2.0.0",
    "@types/ramda": "^0.26.41",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-input-mask": "^2.0.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-select": "^3.0.10",
    "@types/request-promise-native": "^1.0.17",
    "@types/styled-components": "^4.4.2",
    "@types/uuid": "^3.4.7",
    "@types/yup": "^0.26.29",
    "babel-plugin-styled-components": "^1.10.7",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "node-sass": "^4.13.1",
    "nwjs-builder-phoenix": "^1.15.0",
    "nwjs-types": "https://github.com/envis10n/nwjs-types.git",
    "prettier": "^1.19.1",
    "string-replace-loader": "^2.2.0",
    "styled-components": "^5.0.0",
    "stylelint": "^13.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.9.0",
    "typescript": "^3.7.5",
    "wait-on": "^4.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
