{
  "name": "supplier",
  "productName": "Supplier",
  "description": "Supreme Bot",
  "version": "0.9.0",
  "copyright": "Supplier",
  "main": "build/electron.js",
  "homepage": ".",
  "private": true,
  "author": {
    "name": "Supplier",
    "email": "contact@supplierbot.io",
    "url": "https://supplierbot.io"
  },
  "scripts": {
    "start": "NODE_ENV=development yarn build-main && concurrently \"cross-env BROWSER=none craco start\" \"wait-on http://localhost:3000 && electron .\" --kill-others",
    "production": "NODE_ENV=production electron .",
    "build": "NODE_ENV=production yarn build-main && yarn build-renderer && yarn compile",
    "build-main": "yarn build-page-inject && webpack --config src/main/webpack.config.js && cp public/app.js build/app.js && rm -f build/app.jsc",
    "build-renderer": "cross-env GENERATE_SOURCEMAP=false NODE_ENV=production craco build",
    "build-page-inject": "node tools/createInjectSource.js",
    "fetch-browsers": "node tools/fetchBrowsers.js",
    "compile": "node tools/compile.js",
    "prepack": "node tools/prepack.js",
    "postinstall": "patch-package && NODE_ENV=development yarn build-main && electron-builder install-app-deps",
    "package-all": "yarn prepack && NODE_ENV=production electron-builder build -mw",
    "package-mac": "yarn prepack && NODE_ENV=production electron-builder build -m",
    "package-win": "yarn prepack && NODE_ENV=production electron-builder build -w",
    "publish-all": "yarn prepack && NODE_ENV=production electron-builder --win --mac -p always",
    "publish-mac": "yarn prepack && NODE_ENV=production electron-builder --mac -p always",
    "publish-win": "yarn prepack && NODE_ENV=production electron-builder --win -p always"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,css,scss}": [
      "prettier --config .prettierrc --write",
      "eslint --fix",
      "git add"
    ]
  },
  "build": {
    "appId": "com.supplier.bot.io",
    "productName": "Supplier",
    "copyright": "Copyright © 2020 ${author}",
    "afterSign": "tools/notarization/notarize.js",
    "asarUnpack": "./node_modules/**",
    "mac": {
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "tools/notarization/entitlements.mac.plist",
      "entitlementsInherit": "tools/notarization/entitlements.mac.plist",
      "category": "supplier.bot.io",
      "electronUpdaterCompatibility": ">= 2.16"
    },
    "win": {
      "publisherName": "Supplier Patryk Heljasi�ski",
      "target": "nsis",
      "electronUpdaterCompatibility": ">= 2.16"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json",
      "!electron-builder.env"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": {
      "provider": "spaces",
      "name": "supplier",
      "region": "nyc3"
    }
  },
  "dependencies": {
    "@craco/craco": "^5.6.3",
    "@reduxjs/toolkit": "^1.2.5",
    "@types/aes-js": "^3.1.0",
    "@types/crypto-js": "^3.1.43",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "axios": "^0.19.2",
    "bytenode": "^1.1.6",
    "cheerio": "^1.0.0-rc.3",
    "chrome-launcher": "^0.13.4",
    "connected-react-router": "^6.8.0",
    "crypto-js": "^3.1.9-1",
    "discord-rpc": "^3.1.0",
    "discord.js": "^12.2.0",
    "electron-better-ipc": "^1.1.1",
    "electron-notarize": "^1.0.0",
    "electron-updater": "^4.3.8",
    "firebase": "^7.8.0",
    "formik": "^2.1.3",
    "fs-extra": "^8.1.0",
    "global": "^4.4.0",
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "npm-check": "^5.9.2",
    "path-to-regexp": "^6.1.0",
    "playwright-core": "^1.10.0",
    "puppeteer-core": "5",
    "puppeteer-extra": "^3.1.18",
    "puppeteer-extra-plugin-stealth": "^2.7.6",
    "ramda": "^0.27.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.19",
    "react-input-mask": "^3.0.0-alpha.2",
    "react-is": "^16.12.0",
    "react-particles-js": "^2.7.0",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.1",
    "react-select": "^3.0.8",
    "redux-observable": "^1.2.0",
    "redux-persist": "^6.0.0",
    "redux-persist-transform-encrypt": "^2.0.1",
    "redux-saga": "^1.1.3",
    "redux-saga-firebase": "^0.15.0",
    "request-promise-native": "^1.0.8",
    "rxfire": "^3.10.0",
    "rxjs": "^6.5.4",
    "secret-agent": "^1.4.1-alpha.2",
    "semver": "^7.1.3",
    "set-cookie-parser": "^2.4.5",
    "tough-cookie": "^4.0.0",
    "unlazy-loader": "^0.1.3",
    "uuid": "^3.4.0",
    "uuid-random": "^1.3.0",
    "yup": "^0.28.5"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@types/cheerio": "^0.22.16",
    "@types/discord-rpc": "^3.0.2",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/flat": "^5.0.0",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^25.1.1",
    "@types/lodash": "^4.14.149",
    "@types/needle": "^2.0.4",
    "@types/node": "^14.14.37",
    "@types/ramda": "^0.26.41",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-input-mask": "^2.0.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-select": "^3.0.10",
    "@types/redux-immutable-state-invariant": "^2.1.1",
    "@types/request-promise-native": "^1.0.17",
    "@types/semver": "^7.1.0",
    "@types/set-cookie-parser": "^0.0.6",
    "@types/styled-components": "^5.0.1",
    "@types/uuid": "^7.0.2",
    "@types/yup": "^0.26.29",
    "babel-cli": "^6.26.0",
    "babel-loader": "8.1.0",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-minify-mangle-names": "^0.5.0",
    "babel-plugin-styled-components": "^1.10.7",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.0",
    "electron": "^11.4.1",
    "electron-builder": "^22.10.5",
    "electron-devtools-installer": "^3.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "node-sass": "^4.13.1",
    "patch-package": "^6.2.0",
    "postinstall-postinstall": "^2.0.0",
    "prettier": "^1.19.1",
    "string-replace-loader": "^2.2.0",
    "styled-components": "^5.0.0",
    "stylelint": "^13.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.9.0",
    "typescript": "^3.8.2",
    "wait-on": "^4.0.0",
    "webpack-cli": "^3.3.11",
    "webpack-messages": "^2.0.4",
    "webpack-node-externals": "^1.7.2"
  },
  "browserslist": [
    "electron >=11"
  ]
}
